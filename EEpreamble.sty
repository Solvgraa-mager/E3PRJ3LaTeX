}
% #1 Filename
% #2 Caption
\newcommand\tabOC[3]{
	\begin{table*}[!ht]
		\caption{#2}
		\label{tab:#3}
		\renewcommand{\arraystretch}{1.3}
		\centering
			#1
	\end{table*}
}

\newcommand\includecode[4][src]{
    \lstinputlisting[
      label={#1:#2},
      caption={#3},
      xrightmargin=0.05\linewidth,
      breakatwhitespace,
      breaklines,
      #4,
    ]{\dirsrc#2}
}

